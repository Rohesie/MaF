<doctrine-mapping>
        <entity name="BM2\SiteBundle\Entity\SkillType">
                <id name="id" type="integer">
                        <generator strategy="AUTO" />
                </id>

                <field name="name" type="string" />
                <many-to-one field="category" target-entity="SkillCategory" inversed-by="skills"/>
        </entity>
</doctrine-mapping>
