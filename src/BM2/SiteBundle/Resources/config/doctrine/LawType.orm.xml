<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping>
	<entity name="BM2\SiteBundle\Entity\LawType" table="types.lawtype">
		<id name="id" type="integer">
			<generator strategy="IDENTITY"/>
		</id>
		<field name="name" type="string"/>
		<field name="category" type="string"/>
		<field name="allow_multiple" type="boolean"/>
	</entity>
</doctrine-mapping>
