<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping>
	<entity name="BM2\SiteBundle\Entity\SiegeEquipmentType" table="types.siegeequipmenttype">
		<id name="id" type="integer">
			<generator strategy="IDENTITY"/>
		</id>
		<field name="name" type="string"/>
		<field name="ranged" type="boolean"/>
		<field name="hours" type="integer"/>
		<field name="soldiers" type="integer"/>
		<field name="contacts" type="integer"/>
	</entity>
</doctrine-mapping>
