{% block form_errors %}
{% spaceless %}
{% if errors|length > 0 %}
<ul class="error_list">
    {% for error in errors %}
    <li>{{ error.messageTemplate|trans(error.messageParameters, 'validators') }}</li>
    {% endfor %}
</ul>
{% endif %}
{% endspaceless %}
{% endblock form_errors %}

{% block display_widget %}
{% spaceless %}
	{{ form.vars.label|trans }}
{% endspaceless %}
{% endblock %}
